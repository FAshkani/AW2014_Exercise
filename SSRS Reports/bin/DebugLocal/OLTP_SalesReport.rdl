        </QueryParameters>
        <CommandText>SELECT        LineTotal, finYear, OrderQty, SalesOrderID, SalesOrderDetailID, CarrierTrackingNumber, ProductID, SpecialOfferID, UnitPrice, UnitPriceDiscount, ModifiedDate, Product, ProductCategory, ProductSubCategory, 
                         SalesOrderNumber
FROM            v_SalesByProductByCategory
WHERE        (ProductSubCategory IN (@SubCategory)) AND (finYear IN (@Year))</CommandText>
